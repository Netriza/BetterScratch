{
  "manifest_version": 2,
 
  "name": "BetterScratch",
  "version": "1.0.1",
  "description": "Extentions for Scratch",
 
  "content_scripts": [{
    "css": ["style.css"],
    "js": ["jquery-3.5.1.min.js"],
    "matches": ["https://scratch.mit.edu/*"]
  }],
 
    "page_action": {
      "default_popup": "popup.php",
      "default_icon": {
        "16": "get_started16.png",
        "32": "get_started32.png",
        "48": "get_started48.png",
        "128": "get_started128.png"
      }
    },
    "icons": {
      "16": "get_started16.png",
      "32": "get_started32.png",
      "48": "get_started48.png",
      "128": "get_started128.png"
    }
}
